@Article{Yu2007,
  author        = {Yu, Junying and Vodyanik, Maxim A. and Smuga-Otto, Kim and Antosiewicz-Bourget, Jessica and Frane, Jennifer L. and Tian, Shulan and Nie, Jeff and Jonsdottir, Gudrun A. and Ruotti, Victor and Stewart, Ron and Slukvin, Igor I. and Thomson, James A.},
