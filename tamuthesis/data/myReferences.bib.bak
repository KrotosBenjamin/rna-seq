@Unpublished{Hillman2017,
  author   = {Paul R. Hillman and Sarah G. B. Christian and Ryan Doan and Noah D. Cohen and Kranti Konganti and Xu Wang and Kory Douglas and Andrew Clark and Paul B. Samollow and Scott V. Dindot},
  title    = {Genomic imprinting does not reduce {Ube3alevels} expression levels},
  year     = {2017},
  abstract = {The ubiquitin protein E3A ligase gene (UBE3A) gene is imprinted with maternal-specific expression in neurons and biallelically expressed in all other cell types. Both loss of function and gain of function mutations affecting the expression levels of UBE3A are associated with several neurodevelopmental syndromes and psychological conditions, suggesting that UBE3A is dosage sensitive in the brain and that imprinting of UBE3A might have evolved as a dosage regulating mechanism. Why UBE3A is specifically imprinted in neurons and not in other cell types, however, is unknown. To determine whether imprinting of UBE3A functions as a dosage regulating mechanism, we examined UBE3A transcript and protein levels among cells, tisseus, and species with different imprinting states of UBE3A. We found no correlation between the imprinting status and expression levels of UBE3A. Importantly, we found that in mouse, expression of the maternal Ube3a allele is upregulated during neurogenesis, fully compensating for the loss of expression of paternal Ube3a allele. Based on these findings, we propose that imprinting of Ube3a/UBE3A does not function to reduce the dosage of Ube3a/UBE3A in neurons, but rather to regulate some other, yet to be determined, aspect of gene expression or protein function.},
}

